{
  "name": "angularjs-study-app",
  "version": "1.0.0",
  "description": "Simple app to learn AngularJS",
  "main": "index.ts",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nikolay-borzov/angularjs-study-app.git"
  },
  "scripts": {
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dev",
    "build:dev": "webpack --config webpack-dev.config.js",
    "prebuild:prd": "npm run clean:prd",
    "build:prd": "webpack --config webpack-prd.config.js",
    "clean:dev": "rimraf .tmp",
    "clean:prd": "rimraf public",
    "json-server": "json-server --watch db.json --delay 750",
    "start": "run-p serve:dev json-server",
    "serve:dev": "webpack-dev-server --config webpack-dev.config.js",
    "preserve:prd": "npm run build:prd",
    "serve:prd": "http-server",
    "test": "karma start karma.config.js",
    "test:coverage": "remap-istanbul -i build/coverage/coverage.json -o build/coverage/report -t html",
    "test:coveralls": "remap-istanbul -i build/coverage/coverage.json -o build/coverage/lcov.info -t lcovonly",
    "test:coveralls:push": "cat ./build/coverage/lcov.info | ./node_modules/.bin/coveralls",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "tsc -p e2e-tests",
    "protractor": "protractor e2e-tests/tmp/protractor.conf.js"
  },
  "keywords": [
    "study project"
  ],
  "author": "Nikolay Borzov <nikolay.n.borzov@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/angular": "^1.6.53",
    "@types/angular-mocks": "^1.7.0",
    "@types/angular-resource": "^1.5.14",
    "@types/angular-ui-router": "^1.1.40",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "^3.3.5",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "@types/webpack-env": "^1.13.6",
    "ajv": "^6.6.2",
    "angular-animate": "^1.7.5",
    "angular-aria": "^1.7.5",
    "angular-material": "^1.1.12",
    "angular-messages": "^1.7.5",
    "angular-mocks": "^1.7.5",
    "angular-resource": "^1.7.5",
    "awesome-typescript-loader": "^5.2.1",
    "core-js": "^2.6.1",
    "coveralls": "^3.0.2",
    "css-loader": "^2.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine-core": "^3.3.0",
    "json-server": "^0.14.2",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^2.0.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.5",
    "ng-annotate-loader": "^0.6.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "protractor": "^5.4.2",
    "remap-istanbul": "^0.12.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.2.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@types/angular-material": "^1.1.62",
    "@uirouter/angularjs": "^1.0.20",
    "angular": "^1.7.5"
  }
}
