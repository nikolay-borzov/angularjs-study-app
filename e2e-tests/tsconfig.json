{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": false,
    "removeComments": false,
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "tmp",
    "lib": ["es6"],
    "types": ["jasmine", "jasminewd2", "node"]
  },
  "exclude": ["node_modules"]
}
